schrijf "\r\nGeef een heel getal op die vergelijkt moet worden in een zolang loop";

@@ Lees nummer in
zolangNummer < (nummer);

zolang(zolangNummer > 0){
    schrijf "\r\nAantal keer geschreven ";
    schrijf zolangNummer;
    zolangNummer--;
}

schrijf "\r\nGeef een komma getal op voor een zolang loop";

@@ Lees nummer in
zolangVliegendNummer < (vliegend);

zolang(zolangVliegendNummer > 0){
    schrijf "\r\nAantal keer geschreven ";
